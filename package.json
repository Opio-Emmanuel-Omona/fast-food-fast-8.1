{
  "name": "fast-food-fast-8.1",
  "version": "1.0.0",
  "description": "Order you food online",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --port 4000 --config ./webpack.config.js --mode development --open",
    "build": "webpack --mode development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "heroku-postbuild": "npm run build"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/enzyme.config.js",
      "<rootDir>/jest.config.js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/src/store/index.js",
      "<rootDir>/src/index.js",
      "<rootDir>/src/reducers/rootReducer.js",
      "<rootDir>/src/store/index.js",
      "<rootDir>/src/components/App/index.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/setupTests.js",
      "!src/index.js",
      "!**/node_modules/**"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Opio-Emmanuel-Omona/fast-food-fast-8.1.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Opio-Emmanuel-Omona/fast-food-fast-8.1/issues"
  },
  "homepage": "https://github.com/Opio-Emmanuel-Omona/fast-food-fast-8.1#readme",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/polyfill": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.15.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "image-webpack-loader": "^4.6.0",
    "jest": "^23.4.2",
    "react-hot-loader": "^4.7.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "axios": "^0.18.0",
    "express": "^4.16.4",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "react-toastify": "^4.5.2",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0"
  }
}
